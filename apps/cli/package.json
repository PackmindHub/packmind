{
  "name": "@packmind/cli",
  "version": "0.6.0",
  "description": "A command-line interface for Packmind linting and code quality checks",
  "private": false,
  "bin": {
    "packmind-cli": "./main.cjs"
  },
  "main": "./main.cjs",
  "files": [
    "main.cjs",
    "*.wasm",
    "stubs",
    "CHANGELOG.MD"
  ],
  "keywords": [
    "packmind",
    "linting",
    "code-quality",
    "cli",
    "static-analysis"
  ],
  "author": "Packmind",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PackmindHub/packmind.git",
    "directory": "apps/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "packmind-cli": "ts-node --project tsconfig.app.json src/main.ts"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.9",
    "inquirer": "^13.0.2"
  }
}
